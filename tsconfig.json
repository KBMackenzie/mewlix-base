{
  "compilerOptions": {
    "target": "es2016",                                  
    "module": "es6",
    "rootDir": "./src/",
    "removeComments": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
